# 가장 작은수 니까 list말고 break로 끝내는게 빠름 (훨씬 빠름)
def solution(n):
#     hubo_list=[]
#     for i in range(1,n):
#         if n%i==1:
#             hubo_list.append(i)
            
#     answer = min(hubo_list)
    
    answer=0
    for i in range(1,n):
        if n%i==1:
            answer=i
            break
    return answer
